# AGI OS Environment Variables

# API Keys
OPENAI_API_KEY=your_openai_key_here
CLAUDE_API_KEY=your_claude_key_here
DEEPSEEK_API_KEY=your_deepseek_key_here
GEMINI_API_KEY=your_gemini_key_here

# Stripe
STRIPE_SECRET_KEY=your_stripe_secret_key_here
STRIPE_PUBLISHABLE_KEY=your_stripe_publishable_key_here

# Database
DATABASE_URL=postgresql://user:password@localhost:5432/agi_os
ANALYTICS_DB_URL=clickhouse://localhost:9000/analytics

# Message Queue
RABBITMQ_URL=amqp://guest:guest@localhost:5672/
REDIS_URL=redis://localhost:6379/0

# Vector Database
FAISS_INDEX_PATH=/data/faiss_index
PINECONE_API_KEY=your_pinecone_key_here
PINECONE_ENVIRONMENT=us-west1-gcp

# Blockchain
WEB3_PROVIDER_URL=https://polygon-mainnet.infura.io/v3/your_infura_key
BLOCKCHAIN_PRIVATE_KEY=your_private_key_here
SMART_CONTRACT_ADDRESS=your_contract_address_here

# Application Settings
ENVIRONMENT=development
DEBUG=true
LOG_LEVEL=INFO
API_PORT=8000

# RL Agent Settings
RL_MODEL_PATH=/models/rl_agent
RL_TRAINING_ENABLED=true
RL_UPDATE_INTERVAL=86400  # 24 hours in seconds

# Scraping Settings
GITHUB_TOKEN=your_github_token_here
REDDIT_CLIENT_ID=your_reddit_client_id_here
REDDIT_CLIENT_SECRET=your_reddit_client_secret_here

# Marketplace API
MARKETPLACE_API_URL=https://api.loopfactory.ai
MARKETPLACE_API_KEY=your_marketplace_api_key_here

# Monitoring
PROMETHEUS_PORT=9090
SENTRY_DSN=your_sentry_dsn_here

# Feature Flags
ENABLE_DESKTOP_OBSERVATION=false
ENABLE_BLOCKCHAIN_VERIFICATION=false
ENABLE_SYNTHETIC_GENERATION=false

